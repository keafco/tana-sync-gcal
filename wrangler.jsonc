{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "tana-sync-gcal",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"main": "./src/index.ts",
	"vars": {
		"WORKER_ENV": "production"
	},
	"env": {
		"development": {
			"vars": {
				"WORKER_ENV": "development"
			}
		}
	}
}
